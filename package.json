{
  "name": "marina",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn copy-assets && run-p build:**",
    "build:popup": "parcel build src/popup.html --public-url='/'",
    "build:scripts": "parcel build scripts/*.ts",
    "clean": "rimraf dist",
    "copy-assets": "mkdir -p dist && cp -r public/* dist/",
    "lint": "eslint --ext .ts,.tsx src scripts __test__",
    "lint:fix": "eslint --fix --ext .ts,.tsx src scripts __test__",
    "prettier": "prettier --write src scripts",
    "prettier:check": "prettier --check src scripts",
    "run:firefox": "web-ext run --source-dir dist",
    "run:brave-linux": "web-ext run --target chromium --chromium-binary /usr/bin/brave --source-dir dist",
    "run:chrome-linux": "web-ext run --target chromium --chromium-binary /usr/bin/google-chrome-stable --source-dir dist",
    "run:brave-mac": "web-ext run --target chromium --chromium-binary /Applications/Brave\\ Browser.app/Contents/MacOS/Brave\\ Browser --source-dir dist",
    "run:chrome-mac": "web-ext run --target chromium --chromium-binary /Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --source-dir dist",
    "start": "(sleep 5 && yarn run:firefox) & yarn watch",
    "start:brave-mac": "(sleep 5 && yarn run:brave-mac) & yarn watch",
    "start:chrome-mac": "(sleep 5 && yarn run:chrome-mac) & yarn watch",
    "start:brave-linux": "(sleep 5 && yarn run:brave-linux) & yarn watch",
    "start:chrome-linux": "(sleep 5 && yarn run:chrome-linux) & yarn watch",
    "test": "jest",
    "watch": "yarn clean && yarn copy-assets && run-p watch:**",
    "watch:popup": "parcel watch src/popup.html --public-url='/'",
    "watch:background": "parcel watch scripts/background.ts",
    "web-ext:build": "web-ext build --source-dir dist",
    "web-ext:lint": "web-ext lint --source-dir dist"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.0",
    "fp-ts": "^2.9.1",
    "http-status-codes": "^2.1.4",
    "io-ts": "^2.2.13",
    "postcss": "^7.0.35",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.1",
    "webextension-polyfill-ts": "^0.22.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.18",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "jest": "^26.6.3",
    "mockzilla": "^0.9.0",
    "mockzilla-webextension": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "prettier": "^2.2.1",
    "prettier-plugin-tailwind": "^2.2.8",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2",
    "web-ext": "^5.4.1"
  }
}
