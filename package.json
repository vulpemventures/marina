{
  "name": "marina",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run copy-assets && run-p build:**",
    "build:html": "parcel build --no-cache public/*.html",
    "build:scripts": "parcel build --no-cache src/application/background_script.ts",
    "clean": "rimraf dist",
    "copy-assets": "mkdir -p dist && cp public/manifest.json dist/ && cp -r public/assets dist/",
    "lint": "eslint --ext .ts,.tsx src __test__",
    "lint:fix": "eslint --fix --ext .ts,.tsx src __test__",
    "prettier": "prettier --write src __test__",
    "prettier:check": "prettier --check src __test__",
    "run:firefox": "web-ext run --verbose --browser-console --firefox=firefoxdeveloperedition --firefox-profile=dev --source-dir dist",
    "run:brave-linux": "web-ext run --target chromium --chromium-binary '/usr/bin/brave' --source-dir dist",
    "run:chrome-linux": "web-ext run --target chromium --chromium-binary chromium --source-dir dist",
    "run:brave-mac": "web-ext run --target chromium --chromium-binary '/Applications/Brave Browser.app/Contents/MacOS/Brave Browser' --source-dir dist",
    "run:chrome-mac": "web-ext run --target chromium --chromium-binary '/Applications/Google Chrome.app/Contents/MacOS/Google Chrome' --source-dir dist",
    "start": "(sleep 10 && npm run run:firefox) & npm run watch",
    "start:brave-mac": "(sleep 10 && npm run run:brave-mac) & npm run watch",
    "start:chrome-mac": "(sleep 10 && npm run run:chrome-mac) & npm run watch",
    "start:brave-linux": "(sleep 10 && npm run run:brave-linux) & npm run watch",
    "start:chrome-linux": "(sleep 10 && npm run run:chrome-linux) & npm run watch",
    "test": "jest",
    "watch": "npm run clean && npm run copy-assets && run-p watch:**",
    "watch:html": "parcel watch public/*.html",
    "watch:scripts": "parcel watch src/application/background_script.ts",
    "web-ext:build": "web-ext build --source-dir dist",
    "web-ext:lint": "web-ext lint --source-dir dist"
  },
  "dependencies": {
    "@headlessui/react": "^0.2.0",
    "formik": "^2.2.6",
    "ldk": "^0.1.2",
    "postcss-cli": "^7.1.2",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.1",
    "react-combine-reducers": "^1.0.4",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.1.0",
    "react-router-dom": "^5.2.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.0.2",
    "uuid": "^8.3.2",
    "webextension-polyfill-ts": "^0.22.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.20",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^17.0.0",
    "@types/react-combine-reducers": "^1.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "autoprefixer": "^9.8.6",
    "classnames": "^2.2.6",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "jest": "^26.6.3",
    "mockzilla": "^0.9.0",
    "mockzilla-webextension": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss": "^7.0.35",
    "prettier": "^2.2.1",
    "prettier-plugin-tailwind-css": "^1.4.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "web-ext": "^5.5.0"
  }
}
