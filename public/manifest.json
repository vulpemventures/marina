{
  "name": "marina",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Liquid Wallet Web Extension",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "192": "logo192.png"
  },
  "web_accessible_resources": [],
  "browser_action": {
    "default_icon": "logo192.png",
    "default_popup": "popup.html"
  }
}
